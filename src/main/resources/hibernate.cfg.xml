    <property name="connection.url">jdbc:mysql://localhost:3306/dlfm</property>
    <property name="connection.driver_class">com.mysql.jdbc.Driver</property>
      <mapping class="com.sharewin.modules.sys.entity.DlfmBanner"/>
    <mapping class="com.sharewin.modules.sys.entity.DlfmNews"/>
    <mapping class="com.sharewin.modules.sys.entity.Permission"/>
    <mapping class="com.sharewin.modules.sys.entity.Role"/>
    <mapping class="com.sharewin.modules.sys.entity.User"/>
    <mapping class="com.sharewin.modules.sys.entity.DlfmBook"/>
      <mapping class="com.sharewin.modules.sys.entity.DlfmBookEntity"/>
      <mapping class="com.sharewin.modules.sys.entity.Survey"/>
    <mapping class="com.sharewin.modules.sys.entity.SurveyAnswer"/>
    <mapping class="com.sharewin.modules.sys.entity.SurveyQuestionEntity"/>
    <mapping class="com.sharewin.modules.sys.entity.SurveyQuestion"/>
      <mapping class="com.sharewin.modules.sys.entity.DlfmNewsEntity"/>
      <mapping class="com.sharewin.modules.sys.entity.DlfmAct"/>
