      <mapping class="com.sharewin.modules.sys.entity.PermissionEntity"/>
      <mapping class="com.sharewin.modules.sys.entity.DlfmActEntity"/>
      <mapping class="com.sharewin.modules.sys.entity.DlfmBannerEntity"/>
      <mapping class="com.sharewin.modules.sys.entity.DlfmBookPeriod"/>
      <mapping class="com.sharewin.modules.sys.entity.DlfmBookPeriodEntity"/>
    <mapping class="com.sharewin.modules.sys.entity.DlfmActRootEntity"/>
